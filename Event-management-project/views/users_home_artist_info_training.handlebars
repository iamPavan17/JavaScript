<!-- About Section -->

<section id="asdb">
     <div class="container" id="b">
          <div class="row">
               <div class="col-md-offset-1 col-md-10 col-sm-12">
                    <div class="blog-post-thumb">
                        <div class="blog-post-title">
                              <h3><a href="">Training</a></h3>
                         </div><br>
                        <table class="table">
                <thead class="thead-light">
                    <tr>
                        <th scope="col">ID</th>
                        <th scope="col">Timings</th>
                        <th scope="col">Place</th>
                        <th scope="col">Batches</th>
                        <th scope="col">Contact</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each display}}
                        <tr>
                            <td>{{this.id}}</td>
                            <td>{{this.timings}}</td>
                            <td>{{this.address}}</td>
                            <td>{{this.batches}}</td>
                            <td>{{this.contact}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
                         
                         
                    </div>
               </div>

          </div>
     </div>
</section>


<section id="contact">
     <div class="container">
          <div class="row">

               <div class="col-md-offset-1 col-md-10 col-sm-12">
                    <h4><b>Want to book an Appointment?</b></h4> <span>{{msg}}</span>
                    <form action="sendAppointment" method="POST">
                         <div class="col-md-4 col-sm-4">
                              <input name="name" type="text" class="form-control" id="name" placeholder="Name" required>
                         </div>
                         <div class="col-md-4 col-sm-4">
                              <input name="email" type="email" class="form-control" id="email" placeholder="Email" required>
                      	</div>
                         <div class="col-md-4 col-sm-4">
                              <input name="phone" type="text" class="form-control" id="subject" placeholder="Phone" required>
                      	</div>
                         <div class="col-md-4 col-sm-4">
                              <input name="date" type="date" class="form-control" id="subject" placeholder="Date" required>
                      	</div>
                         <div class="col-md-4 col-sm-4">
                              <input name="time" type="number" class="form-control" id="subject" placeholder="Time" min="1" max="12" required>
                              {{!-- <select name="" id="" class="form-control">
                                   <option value="1">1</option>
                                   <option value="1">1</option>
                              </select> --}}
                      	</div>
                           <div class="col-md-4 col-sm-4">
                              <select name="format" id="" class="form-control">
                                   <option value="AM">AM</option>
                                   <option value="PM">PM</option>
                              </select>
                      	</div>
                         <div class="col-md-12 col-sm-12">
                              <textarea name="message" rows="5" class="form-control" id="message" placeholder="Message"></textarea>
                         </div>
                         <div class="col-md-3 col-sm-6">
                              <input name="submit" type="submit" class="form-control" id="submit" value="Send"> 
                         </div>
                    </form>
               </div>

          </div>
     </div>
</section>

