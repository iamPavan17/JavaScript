<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <a class="navbar-brand pl-4" href="#">Welcome {{display.ArtistNama}}!</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse pl-5" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto pl-5">
          <li class="nav-item pl-2">
            <a class="nav-link" href="/norartist_home">Bio</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/norartist_home_performance">Performance list</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="#">Gallery</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/norartist_home_schedule">Schedule</a>
