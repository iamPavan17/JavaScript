<script type="text/javascript">
      document.getElementById('rating5').addEventListener('click', function() {
        document.getElementById('value').innerHTML = 'Excellent';
      });
      document.getElementById('rating4').addEventListener('click', function() {
        document.getElementById('value').innerHTML = 'Very Good';
      });
      document.getElementById('rating3').addEventListener('click', function() {
        document.getElementById('value').innerHTML = 'OK';
      });
      document.getElementById('rating2').addEventListener('click', function() {
        document.getElementById('value').innerHTML = 'Bad';
      });
