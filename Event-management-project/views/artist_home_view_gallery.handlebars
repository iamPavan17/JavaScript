    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css">
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed" rel="stylesheet">
    <link rel='stylesheet' href='https://use.fontawesome.com/releases/v5.7.0/css/all.css' integrity='sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ' crossorigin='anonymous'>
</head>
<body>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <a class="navbar-brand pl-4" href="#">Welcome {{display.username}}!</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse pl-5" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto pl-5">
          <ul class="navbar-nav mr-auto pl-5">
          <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home">Bio</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_performance">Performance list</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link" href="/artist_home_gallery">Gallery <span class="sr-only">(current)</span></a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_schedule">Schedule</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="#">Training </a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_invite_user">Invite Artist</a>
          </li>
           <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_list_artist">List of Accepted or Pending Artists</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_appointment">Appointment  </a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_reset_password">Reset Password</a>
          </li><li class="nav-item pl-2">
            <a class="nav-link" href="/artist_home_reviews">Reviews</a>
          </li>
          <li class="nav-item pl-2">
            <a class="nav-link" href="/logout">Logout</a>
          </li>
        </ul>
      </div>
    </nav>
    <br><br><br>
    <div class="container p-5" style="border: 1px solid rgb(218, 213, 213); border-radius: 5px;">
          <div class="row">
              {{#each display}}
              <div class="col-4">
                  <div class="card" style="width: 18rem;">
                    <img src="../public/img/{{this.image_name}}" class="card-img-top" alt="...">
                    <div class="card-body">
                      <p class="card-text">{{this.caption}}.</p>
                    </div>
                  </div>
              </div>
              {{/each}}
          </div>
    </div>
      
